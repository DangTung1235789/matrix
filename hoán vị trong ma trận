#include<iostream>
using namespace std;
int main() {
	int i, j, n, m;
	int a[10][10];
	cout << " nhap n ";
	cin >> n;
	cout << " nhap m ";
	cin >> m;
	for (i = 1;i <= n;i++) {
		for (j = 1;j <= m;j++) {
			cout << " nhap a[" << i << "][" << j << "]=";
			cin >> a[i][j];

		}
	}
	for (i = 1;i <= n;i++) {
		for (j = 1;j <= m;j++) {
			cout << " " << a[i][j];
		}
		cout << endl;
	}
	cout << "========="<<endl;
	for (j = 1;j <= m;j++) {
		for (i = 1;i <= n;i++) {
			cout << " " << a[i][j];
		}
		cout << endl;
	}
}
